MoveWatcher()
{
	set-signal-mask SIG_MOVE;
	while( TRUE )
	{
		moving = (get CURRENT_SPEED) > 5 OR direction;
		sleep 100;
	}
	return;
}

MeleeControl()
{
	while( TRUE )
	{
		if( moving )
		{
			if( firing )
			{
				call-script walk_legs();
			}
			else if( get IN_WATER )
			{
				call-script walk_water();
			}
			else if( get ON_ROAD )
			{
				call-script walk_road();
			}
			else
			{
				call-script walk();
			}
			restored = FALSE;
			legs_restored = FALSE;
		}
		else if( !busy_dying )
		{
			if( !legs_restored )
			{
				call-script restore_legs();
				legs_restored = TRUE;
			}
			else if( ( !restored ) AND ( !aiming ) )
			{
				call-script restore_x();
				restored = TRUE;
			}
			else
			{
				sleep 100;
			}
		}
		else
		{
			sleep 100;
		}
	}
	return;
}

StatusControl()
{
	var Func_Var_1;
	Func_Var_1 = 0;
	while( Func_Var_1 < 2 )
	{
		vetlevel = get VETERAN_LEVEL;
		if( Func_Var_1 == 0 )
		{
			if( vetlevel > 4 )
			{
				show spines_5;
				++Func_Var_1;
			}
		}
		else if( Func_Var_1 == 1 )
		{
			if( vetlevel > 9 )
			{
				show spade_10;
				show staff_10;
				++Func_Var_1;
			}
		}
		sleep 1000;
	}
	return;
}

Create()
{
	hide spines_5;
	hide staff_10;
	hide spade_10;
	moving = FALSE;
	aiming = FALSE;
	firing = FALSE;
	busy_dying = FALSE;
	Static_Var_5 = 0;
	restored = FALSE;
	legs_restored = FALSE;
	vetlevel = 0;
	direction = 0;
	Static_Var_10 = 0;
	restore_delay = 5000;
	while( get BUILD_PERCENT_LEFT )
	{
		sleep 100;
	}
	start-script MoveWatcher();
	start-script MeleeControl();
	start-script StatusControl();
	return;
}

TurnDirection(dir)
{
	direction = dir;
	return;
}

SetMaxReloadTime(time)
{
	restore_delay = time * 2;
	return;
}

RestoreAfterDelay()
{
	signal SIG_RESTORE;
	set-signal-mask SIG_RESTORE;
	var Func_Var_1;
	Func_Var_1 = 0;
	while( Func_Var_1 < restore_delay )
	{
		if( Static_Var_5 )
		{
			Func_Var_1 = restore_delay;
		}
		else
		{
			Func_Var_1 = Func_Var_1 + 100;
		}
		sleep 100;
	}
	while( firing )
	{
		sleep 100;
	}
	set WEAPON_AIM_ABORTED to Static_Var_10;
	aiming = FALSE;
	return;
}

AimWeapon(heading, pitch, weaponnum)
{
	aiming = TRUE;
	signal SIG_AIM;
	set-signal-mask SIG_AIM;
	while( firing )
	{
		sleep 100;
	}
	Static_Var_10 = weaponnum;
	start-script RestoreAfterDelay();
	set WEAPON_READY to weaponnum;
	return;
}

FireWeapon(weaponnum)
{
	firing = TRUE;
	Static_Var_5 = 0;
	if( weaponnum == 0 )
	{
		call-script attack1(weaponnum);
	}
	else if( weaponnum == 1 )
	{
		call-script attack2(weaponnum);
	}
	else
	{
		call-script attack3(weaponnum);
	}
	firing = FALSE;
	return;
}

TargetCleared(weaponnum)
{
	set WEAPON_AIM_ABORTED to weaponnum;
	signal SIG_AIM;
	return;
}

QueryWeapon(piecenum, weaponnum)
{
	piecenum = lizhead;
	return;
}

SweetSpot(piecenum)
{
	piecenum = base;
	return;
}

QueryBlood(piecenum)
{
	piecenum = 3 + Rand( 0, 3 );
	return;
}

Dying(damagetype)
{
	set-signal-mask 0;
	signal SIG_AIM;
	signal SIG_MOVE;
	signal SIG_RESTORE;
	moving = FALSE;
	busy_dying = TRUE;
	if( Rand( 1, 4 ) == 1 )
	{
		play-sound( "TARLICHDIE1", PLAYSOUND_PRIORITY_BATTLE );
	}
	set MAGIC_DEATH to 1;
	return;
}

Killed(severity, corpsetype, damagetype)
{
	var Func_Var_4;
	corpsetype = 0;
	Func_Var_4 = 0;
	while( Func_Var_4 < 3 )
	{
		emit-sfx 256 | 4 from luparm;
		emit-sfx 256 | 4 from ruparm;
		emit-sfx 256 | 4 from head;
		emit-sfx 256 | 4 from neck;
		emit-sfx 256 | 4 from lizFLupleg;
		emit-sfx 256 | 4 from lizFRupleg;
		emit-sfx 256 | 4 from lizRLupleg;
		emit-sfx 256 | 4 from lizRRupleg;
		explode luparm type EXPTYPE_SMALLEXPLOSION | BITMAPONLY;
		explode ruparm type EXPTYPE_SMALLEXPLOSION | BITMAPONLY;
		explode head type EXPTYPE_SMALLEXPLOSION | BITMAPONLY;
		explode neck type EXPTYPE_SMALLEXPLOSION | BITMAPONLY;
		explode lizFLupleg type EXPTYPE_SMALLEXPLOSION | BITMAPONLY;
		explode lizFRupleg type EXPTYPE_SMALLEXPLOSION | BITMAPONLY;
		explode lizRLupleg type EXPTYPE_SMALLEXPLOSION | BITMAPONLY;
		explode lizRRupleg type EXPTYPE_SMALLEXPLOSION | BITMAPONLY;
		++Func_Var_4;
		sleep 480;
	}
	return;
}
