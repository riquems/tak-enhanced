MoveWatcher()
{
	set-signal-mask SIG_MOVE;
	while( TRUE )
	{
		moving = (get CURRENT_SPEED) > 5 OR direction;
		sleep 100;
	}
	return;
}

MeleeControl()
{
	while( TRUE )
	{
		if( moving )
		{
			if( firing )
			{
				call-script walk_legs();
			}
			else if( get IN_WATER )
			{
				call-script walk_water();
			}
			else if( get ON_ROAD )
			{
				call-script walk_road();
			}
			else
			{
				call-script walk();
			}
			legs_restored = FALSE;
			restored = FALSE;
		}
		else if( !busy_dying )
		{
			if( !legs_restored )
			{
				call-script restore_legs();
				legs_restored = TRUE;
			}
			else if( ( !restored ) AND ( !aiming ) AND ( !building ) )
			{
				call-script restore_x();
				restored = TRUE;
			}
			else
			{
				sleep 100;
			}
		}
		else
		{
			sleep 100;
		}
	}
	return;
}

Create()
{
	move emitdan to x-axis [-3.000000] now;
	move emitdan to y-axis [15.000000] now;
	move emitdan to z-axis [29.000000] now;
	move emitdan2 to x-axis [0.000000] now;
	move emitdan2 to y-axis [34.000000] now;
	move emitdan2 to z-axis [7.000000] now;
	moving = FALSE;
	aiming = FALSE;
	firing = FALSE;
	building = FALSE;
	busy_dying = FALSE;
	kill_attack = FALSE;
	legs_restored = FALSE;
	restored = FALSE;
	direction = 0;
	Static_Var_10 = 0;
	restore_delay = 5000;
	while( get BUILD_PERCENT_LEFT )
	{
		sleep 100;
	}
	start-script MoveWatcher();
	start-script MeleeControl();
	return;
}

StartBuilding()
{
	set INBUILDSTANCE to 1;
	building = TRUE;
	if( !aiming )
	{
		call-script startbuild();
		restored = FALSE;
	}
	return;
}

StopBuilding()
{
	set INBUILDSTANCE to 0;
	building = FALSE;
	return;
}

TurnDirection(dir)
{
	direction = dir;
	return;
}

SetMaxReloadTime(time)
{
	restore_delay = time * 2;
	return;
}

RestoreAfterDelay()
{
	signal SIG_RESTORE;
	set-signal-mask SIG_RESTORE;
	var Func_Var_1;
	Func_Var_1 = 0;
	while( Func_Var_1 < restore_delay )
	{
		if( kill_attack )
		{
			Func_Var_1 = restore_delay;
		}
		else
		{
			Func_Var_1 = Func_Var_1 + 100;
		}
		sleep 100;
	}
	while( firing )
	{
		sleep 100;
	}
	set WEAPON_AIM_ABORTED to Static_Var_10;
	aiming = FALSE;
	if( building )
	{
		call-script startbuild();
		restored = FALSE;
	}
	return;
}

AimWeapon(heading, pitch, weaponnum)
{
	signal SIG_AIM;
	set-signal-mask SIG_AIM;
	aiming = TRUE;
	while( firing )
	{
		sleep 100;
	}
	if( building )
	{
		call-script restore_x();
		sleep 250;
	}
	Static_Var_10 = weaponnum;
	start-script RestoreAfterDelay();
	set WEAPON_READY to weaponnum;
	return;
}

FireWeapon(weaponnum)
{
	firing = TRUE;
	kill_attack = FALSE;
	if( weaponnum == 0 )
	{
		call-script attack1(weaponnum);
	}
	else if( weaponnum == 1 )
	{
		call-script attack2(weaponnum);
	}
	else
	{
		call-script attack3(weaponnum);
	}
	firing = FALSE;
	return;
}

TargetCleared(weaponnum)
{
	set WEAPON_AIM_ABORTED to weaponnum;
	kill_attack = TRUE;
	signal SIG_AIM;
	return;
}

QueryWeapon(piecenum, weaponnum)
{
	if( weaponnum == 1 )
	{
		piecenum = emitdan;
	}
	else
	{
		piecenum = emitdan2;
	}
	return;
}

SweetSpot(piecenum)
{
	piecenum = torso;
	return;
}

QueryBlood(piecenum)
{
	piecenum = 3 + Rand( 0, 2 );
	return;
}

Dying()
{
	set-signal-mask 0;
	signal SIG_AIM;
	signal SIG_MOVE;
	signal SIG_RESTORE;
	moving = FALSE;
	busy_dying = TRUE;
	set MAGIC_DEATH to 1;
	return;
}

Killed(severity, corpsetype, damagetype)
{
	corpsetype = 0;
	if( damagetype == 3 )
	{
		explode Head type FALL;
		explode ArmUR type FALL;
		explode ArmUL type FALL;
		explode ArmLR type FALL;
		explode ArmLL type FALL;
		explode torso type FALL | EXPTYPE_BLOODEXPLOSION;
		explode LegUL type FALL;
		explode LegUR type FALL;
		explode HandR type FALL;
		explode HandL type FALL;
		explode FootL type FALL;
		explode FootR type FALL;
		explode LegLL type FALL;
		explode LegLR type FALL;
	}
	return;
}
