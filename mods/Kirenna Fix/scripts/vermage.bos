			hide Cape3;
			hide Cape4;
			mermaid_mode = TRUE;
		}
		else if( ( !in_water ) AND mermaid_mode )
		{
			call-script unswim();
			emit-sfx 256 | 7 from Head;
			emit-sfx 256 | 7 from Cape1;
			emit-sfx 256 | 7 from torso;
			emit-sfx 256 | 7 from FootL;
			show Cape1;
			show Cape2;
			show Cape3;
			show Cape4;
			show LegUR;
			show LegLR;
			show FootR;
			show LegUL;
			show LegLL;
			show FootL;
			hide HideLegU;
			hide HideLegL;
			hide HideTorso;
			hide HideHip;
			hide HideFoot;
			mermaid_mode = FALSE;
		}
		if( moving AND ( !in_water ) )
		{
			if (aiming)
			{	
				call-script walk_legs();
			}
			else if( get UNKNOWN_UNIT_VALUE(34) )
			{
				call-script walk_road();
			}
			else
			{
				call-script walk();
			}
			preparedswim = FALSE;
			legs_restored = FALSE;
			restored = FALSE;
		}
		else if( moving AND in_water )
		{
			if( !preparedswim )
			{
				turn Hip to x-axis <20.000000> speed <130.027473>;
				call-script toswim();
				wait-for-turn Hip around x-axis;
				preparedswim = TRUE;
			}
			call-script swim();
			kill_attack = FALSE;
			legs_restored = FALSE;
			restored = FALSE;
		}
		else if( ( !moving ) AND in_water )
		{
			turn Hip to x-axis <0.000000> speed <60.005495>;
			if( aiming OR building )
			{
				call-script tread_legs();
			}
			else
			{
				call-script tread();
			}
			
			preparedswim = FALSE;
			legs_restored = FALSE;
			restored = FALSE;
		}
		else if( !busy_dying )
		{
			if( !legs_restored )
			{
				call-script restore_legs();
				preparedswim = FALSE;
				legs_restored = TRUE;
			}
			else if( ( !restored ) AND ( !aiming ) AND !building)
			{
				call-script restore_x();
				preparedswim = FALSE;
				restored = TRUE;
			}
			else
			{
				sleep 10;
			}
		}
		else
		{
			sleep 10;
		}
	}
}

WakeControl()
{
	set-signal-mask SIG_WAKE;
	while( TRUE )
	{
		if( get UNKNOWN_UNIT_VALUE(28) AND moving )
		{
			emit-sfx 4 from WakeL;
			emit-sfx 2 from WakeR;
		}
		sleep 300;
	}
}

Create()
{
	hide HideLegU;
	hide HideLegL;
	hide HideTorso;
	hide HideHip;
	hide HideFoot;
	move emitjim to x-axis [3.500000] now;
	move emitjim to y-axis [23.000000] now;
	move emitjim to z-axis [31.000000] now;
	move emitjim2 to x-axis [0.000000] now;
	move emitjim2 to y-axis [30.000000] now;
	move emitjim2 to z-axis [28.000000] now;
	firing = FALSE;
	kill_attack = FALSE;
	preparedswim = FALSE;
	pause_loop = FALSE;
	building = FALSE;
	busy_dying = FALSE;
	aiming = FALSE;
	mermaid_mode = 0;
	in_water = FALSE;
	moving = FALSE;
	weapon = 0;
	restore_delay = 5000;
	direction = 0;
	while( get UNKNOWN_UNIT_VALUE(17) )
	{
		sleep 100;
	}
	start-script MoveWatcher();
	start-script MoveControl();
	start-script WakeControl();
	start-script BuildControl();
}

TurnDirection(Func_Var_1)
{
	direction = Func_Var_1;
}

SetMaxReloadTime(Func_Var_1)
{
	restore_delay = Func_Var_1 * 2;
}

RestoreAfterDelay()
{
	signal SIG_RESTORE;
	set-signal-mask SIG_RESTORE;
	var Func_Var_1;
	Func_Var_1 = 0;
	while( Func_Var_1 < restore_delay )
	{
		if( kill_attack )
		{
			Func_Var_1 = restore_delay;
		}
		else
		{
			Func_Var_1 = Func_Var_1 + 100;
		}
		sleep 100;
	}
	while( firing )
	{
		sleep 100;
	}
	set UNKNOWN_UNIT_VALUE(21) to weapon;
	aiming = FALSE;
}

AimWeapon(Func_Var_1, Func_Var_2, Func_Var_3)
{
	signal SIG_AIM;
	set-signal-mask SIG_AIM;
	aiming = TRUE;
	weapon = Func_Var_3;
	while( firing )
	{
		sleep 100;
	}
	start-script RestoreAfterDelay();
	set UNKNOWN_UNIT_VALUE(22) to Func_Var_3;
}

FireWeapon(Func_Var_1)
{
	firing = TRUE;
	kill_attack = FALSE;
	if( Func_Var_1 == 0 )
	{
		call-script attack1(Func_Var_1);
	}
	else if( Func_Var_1 == 1 )
	{
		call-script attack2(Func_Var_1);
	}
	else
	{
		call-script attack3(Func_Var_1);
	}
	firing = FALSE;
}

TargetCleared(Func_Var_1)
{
	set UNKNOWN_UNIT_VALUE(21) to Func_Var_1;
	kill_attack = TRUE;
	signal SIG_AIM;
}

QueryWeapon(Func_Var_1, Func_Var_2)
{
	if( Func_Var_2 == 1 )
	{
		Func_Var_1 = 2;
	}
	else
	{
		Func_Var_1 = 1;
	}
}

SweetSpot(Func_Var_1)
{
	Func_Var_1 = 0;
}

QueryBlood(Func_Var_1)
{
	Func_Var_1 = 3 + Rand( 0, 2 );
}

Dying()
{
	set-signal-mask 0;
	signal SIG_AIM;
	signal SIG_MOVE;
	signal SIG_RESTORE;
	busy_dying = TRUE;
	set UNKNOWN_UNIT_VALUE(31) to 1;
}

Killed(Func_Var_1, Func_Var_2, Func_Var_3)
{
	Func_Var_2 = 0;
	if( Func_Var_3 == 3 )
	{
		explode ArmUR type FALL;
		explode ArmLR type FALL;
		explode HandR type FALL;
		explode ArmUL type FALL;
		explode ArmLL type FALL;
		explode HandL type FALL;
		explode torso type FALL | EXPTYPE_BLOODEXPLOSION;
		explode LegUR type FALL;
		explode LegLR type FALL;
		explode FootR type FALL;
		explode LegUL type FALL;
		explode LegLL type FALL;
		explode FootL type FALL;
		explode Hip type FALL;
	}
}