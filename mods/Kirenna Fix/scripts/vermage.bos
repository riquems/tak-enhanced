BuildControl()
{
	while( TRUE )
	{
		if( building AND ( !aiming ) )
		{
			while( pause_loop )
			{
				sleep 100;
			}
			
			if (in_water) {
				call-script build_water();
			}
			else {
				call-script build();
			}
		}
		else
		{
			sleep 100;
		}
	}
	return;
}

StartBuilding()
{
	set INBUILDSTANCE to 1;
	building = TRUE;
	if( !aiming )
	{
		pause_loop = TRUE;
		call-script startbuild();
		pause_loop = FALSE;
		restored = FALSE;
	}
	return;
}

StopBuilding()
{
	set INBUILDSTANCE to 0;
	building = FALSE;
	return;
}

MoveWatcher()
{
	set-signal-mask SIG_MOVE;
	while( TRUE )
	{
		in_water = get UNKNOWN_UNIT_VALUE(28);
		moving = (get UNKNOWN_UNIT_VALUE(29) > 5) OR direction;
		sleep 100;
	}
}

MoveControl()
